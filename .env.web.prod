# Production Auth Configuration
# These values are used by docker-compose.prod.yml as build args for the web frontend

# In production, disable local email/password authentication
VITE_ENABLE_LOCAL_AUTH=0

# Enable Google OAuth (production-ready)
VITE_ENABLE_GOOGLE_OAUTH=1

# GitHub OAuth (set to 1 when backend GitHub OAuth is configured)
VITE_ENABLE_GITHUB_OAUTH=0
