{
  "dashboard": {
    "uid": "ml-suggest-3311af",
    "title": "LedgerMind — ML Suggestions",
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "templating": {
      "list": [
        {
          "name": "prom",
          "label": "Prometheus",
          "type": "datasource",
          "query": "prometheus",
          "refresh": 1
        },
        {
          "name": "api",
          "label": "JSON API (optional)",
          "type": "datasource",
          "query": "simpod-json-datasource",
          "refresh": 1,
          "hide": 2
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Validation F1 (last run)",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "lm_ml_train_val_f1_macro",
            "refId": "A",
            "legendFormat": "F1"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": [
              "lastNotNull"
            ],
            "fields": "",
            "values": false
          },
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "orange",
                  "value": 0.65
                },
                {
                  "color": "green",
                  "value": 0.72
                }
              ]
            }
          },
          "overrides": []
        }
      },
      {
        "id": 2,
        "type": "timeseries",
        "title": "Predict Requests by Availability (rps)",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 0
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "sum by (available) (rate(lm_ml_predict_requests_total[5m]))",
            "refId": "A",
            "legendFormat": "available={{available}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "req/s"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 3,
        "type": "gauge",
        "title": "Agreement Rate (rule ↔ model, 30m)",
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 6
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "sum(rate(lm_suggest_compare_total{agree=\"True\"}[30m])) / clamp_min(sum(rate(lm_suggest_compare_total[30m])), 1e-9)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "orange",
                  "value": 0.6
                },
                {
                  "color": "green",
                  "value": 0.75
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "calcs": [
              "lastNotNull"
            ],
            "fields": "",
            "values": false
          }
        }
      },
      {
        "id": 4,
        "type": "timeseries",
        "title": "F1 Macro Over Time (last 7d)",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 8
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "lm_ml_train_val_f1_macro",
            "refId": "A",
            "legendFormat": "F1"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Training Runs by Status",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "sum by (status) (rate(lm_ml_train_runs_total[30m]))",
            "refId": "A",
            "legendFormat": "{{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Prediction Latency (P50, P95, P99)",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(lm_ml_predict_latency_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(lm_ml_predict_latency_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(lm_ml_predict_latency_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Suggestions by Source (rule vs model)",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "sum by (source) (rate(lm_suggest_source_total[5m]))",
            "refId": "A",
            "legendFormat": "{{source}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "req/s"
          },
          "overrides": []
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right"
          }
        }
      },
      {
        "id": 8,
        "type": "stat",
        "title": "Model Availability",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 24
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "sum(rate(lm_ml_predict_requests_total{available=\"True\"}[5m])) / clamp_min(sum(rate(lm_ml_predict_requests_total[5m])), 1e-9)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "orange",
                  "value": 0.95
                },
                {
                  "color": "green",
                  "value": 0.99
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "calcs": [
              "lastNotNull"
            ],
            "fields": "",
            "values": false
          },
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 9,
        "type": "stat",
        "title": "Shadow Mode Agreement (24h)",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 24
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${prom}"
        },
        "targets": [
          {
            "expr": "sum(increase(lm_suggest_compare_total{agree=\"True\"}[24h])) / clamp_min(sum(increase(lm_suggest_compare_total[24h])), 1)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "orange",
                  "value": 0.6
                },
                {
                  "color": "green",
                  "value": 0.75
                }
              ]
            }
          },
          "overrides": []
        },
        "options": {
          "reduceOptions": {
            "calcs": [
              "lastNotNull"
            ],
            "fields": "",
            "values": false
          },
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        }
      }
    ]
  },
  "overwrite": true,
  "folderUid": ""
}
